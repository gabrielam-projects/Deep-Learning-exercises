"        for _ in range(num_hidden_layers - 1):\n","            layers.append(nn.Linear(hidden_size, hidden_size))\n","            layers.append(nn.ReLU())\n","\n","        # Capa de salida\n","        layers.append(nn.Linear(hidden_size, output_size))\n","\n","        # Combinar todas las capas en un Sequential\n","        self.network = nn.Sequential(*layers)\n","\n","    def forward(self, x):\n","        x = x.view(x.size(0), -1)  # Aplanar el tensor de entrada\n","        return self.network(x)"],"metadata":{"id":"Q1ELzg7t4nNT"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Función para entrenar el modelo\n","def train_mlp(num_hidden_layers, epochs=5):\n","    model = MultiLayerPerceptron(input_size=28*28, hidden_size=100, num_hidden_layers=num_hidden_layers, output_size=10)\n","    criterion = nn.CrossEntropyLoss()\n","    optimizer = optim.Adam(model.parameters(), lr=0.001)\n","\n","    train_errors = []\n","    for epoch in range(epochs):\n","        model.train()\n","        running_loss = 0.0\n","        for images, labels in train_loader:\n","            optimizer.zero_grad()\n","            outputs = model(images)\n","            loss = criterion(outputs, labels)\n","            loss.backward()\n","            optimizer.step()\n","            running_loss += loss.item()\n","\n","        avg_train_loss = running_loss / len(train_loader)\n","        train_errors.append(avg_train_loss)\n","\n","        print(f\"Epoch [{epoch+1}/{epochs}], Pérdida de Entrenamiento: {avg_train_loss:.4f}\")\n","\n","    return model, train_errors"],"metadata":{"id":"cJXaa6zQZFuf"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Función para evaluar en el conjunto de prueba\n","def evaluate_test_model_mlp(model):\n","    model.eval()\n","    test_loss = 0.0\n","    criterion = nn.CrossEntropyLoss()\n","    with torch.no_grad():\n","        for images, labels in test_loader:\n","            outputs = model(images)\n","            loss = criterion(outputs, labels)\n","            test_loss += loss.item()\n","\n","    avg_test_loss = test_loss / len(test_loader)\n","    return avg_test_loss\n","\n","# Entrenar y evaluar modelos con diferentes números de capas ocultas\n","hidden_layer_counts = [2, 4, 8, 12, 20]\n","train_errors_final_mlp = []\n","test_errors_final_mlp = []\n","\n","for num_hidden_layers in hidden_layer_counts:\n","    print(f\"\\nEntrenando modelo con {num_hidden_layers} capas ocultas\")\n","\n","    # Entrenar el modelo con el número de capas ocultas especificado\n","    model, train_errors = train_mlp(num_hidden_layers)\n","\n","    # Guardar el error de entrenamiento\n","    train_errors_final_mlp.append(min(train_errors))\n","\n","    # Evaluar el modelo en el conjunto de prueba\n","    test_loss = evaluate_test_model_mlp(model)\n","    test_errors_final_mlp.append(test_loss)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"M-mtHfjq_yUE","executionInfo":{"status":"ok","timestamp":1727154487609,"user_tz":360,"elapsed":577648,"user":{"displayName":"Gabriela Martínez","userId":"02807641605184715686"}},"outputId":"8e4d6349-4630-41c3-9515-064143a35813"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Entrenando modelo con 2 capas ocultas\n","Epoch [1/5], Pérdida de Entrenamiento: 0.2661\n","Epoch [2/5], Pérdida de Entrenamiento: 0.1112\n","Epoch [3/5], Pérdida de Entrenamiento: 0.0833\n","Epoch [4/5], Pérdida de Entrenamiento: 0.0628\n","Epoch [5/5], Pérdida de Entrenamiento: 0.0505\n","\n","Entrenando modelo con 4 capas ocultas\n","Epoch [1/5], Pérdida de Entrenamiento: 0.2983\n","Epoch [2/5], Pérdida de Entrenamiento: 0.1248\n","Epoch [3/5], Pérdida de Entrenamiento: 0.0941\n","Epoch [4/5], Pérdida de Entrenamiento: 0.0731\n","Epoch [5/5], Pérdida de Entrenamiento: 0.0622\n","\n","Entrenando modelo con 8 capas ocultas\n","Epoch [1/5], Pérdida de Entrenamiento: 0.4645\n","Epoch [2/5], Pérdida de Entrenamiento: 0.1759\n","Epo