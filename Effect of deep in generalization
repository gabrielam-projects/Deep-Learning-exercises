)\n","        self.fc1 = nn.Linear(input_size, hidden_size)  # Capa oculta\n","        self.fc2 = nn.Linear(hidden_size, output_size) # Capa de salida\n","\n","    def forward(self, x):\n","        x = x.view(-1, 28*28)     # Aplanar las imágenes de 28x28 a un vector de 784\n","        x = F.relu(self.fc1(x))   # Activación ReLU en la capa oculta\n","        x = self.fc2(x)           # Salida de la capa final\n","        return x\n"],"metadata":{"id":"d1cyLWzjuezM"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["class NeuralNet(nn.Module):\n","    def __init__(self, input_size, hidden_size, output_size):\n","        super(NeuralNet, self).__init__()\n","        self.hidden = nn.Linear(input_size, hidden_size)\n","        self.relu = nn.ReLU()\n","        self.output = nn.Linear(hidden_size, output_size)\n","\n","    def forward(self, x):\n","        x = x.view(x.size(0), -1)  # Aplanar el tensor de entrada\n","        x = self.relu(self.hidden(x))\n","        x = self.output(x)\n","        return x"],"metadata":{"id":"_R8RFkRIcruS"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Entrenamiento del modelo\n","def train_single_hidden_layer_full_train_set(hidden_size, epochs=5):\n","    model = NeuralNet(input_size=28*28, hidden_size=hidden_size, output_size=10)\n","    criterion = nn.CrossEntropyLoss()\n","    optimizer = optim.Adam(model.parameters(), lr=0.001)\n","\n","    train_errors = []\n","    for epoch in range(epochs):\n","        model.train()\n","        running_loss = 0.0\n","        for images, labels in train_loader:  # Usar el conjunto completo de entrenamiento\n","            optimizer.zero_grad()\n","            outputs = model(images)\n","            loss = criterion(outputs, labels)\n","            loss.backward()\n","            optimizer.step()\n","            running_loss += loss.item()\n","\n","        avg_train_loss = running_loss / len(train_loader)\n","        train_errors.append(avg_train_loss)\n","\n","        print(f\"Epoch [{epoch+1}/{epochs}], Pérdida de Entrenamiento: {avg_train_loss:.4f}\")\n","\n","    return model, train_errors"],"metadata":{"id":"dYKZdxLoczL6"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Función para evaluar el modelo en el conjunto de prueba\n","def evaluate_test_model(model):\n","    model.eval()\n","    test_loss = 0.0\n","    criterion = nn.CrossEntropyLoss()\n","    with torch.no_grad():\n","        for images, labels in test_loader:\n","            outputs = model(images)\n","            loss = criterion(outputs, labels)\n","            test_loss += loss.item()\n","\n","    avg_test_loss = test_loss / len(test_loader)\n","    return avg_test_loss\n","\n","# Entrenar y evaluar modelos con diferentes tamaños de la capa oculta\n","hidden_sizes = [100, 200, 500, 800, 1000, 1300, 1600, 2000]\n","train_errors_final = []\n","test_errors_final = []\n","\n","for hidden_size in hidden_sizes:\n","    print(f\"\\nEntrenando modelo con {hidden_size} neuronas en la capa oculta\")\n","\n","    # Entrenar el modelo con el conjunto completo de entrenamiento\n","    model, train_errors = train_single_hidden_layer_full_train_set(hidden_size)\n","\n","    # Guardar el error de entrenamiento\n","    train_errors_final.append(min(train_errors))\n","\n","    # Evaluar el modelo en el conjunto de prueba\n","    test_loss = evaluate_test_model(model)\n","    test_errors_final.append(test_loss)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"fDJMTJazdGV9","executionInfo":{"status":"ok","timestamp":1727153566685,"user_tz":360,"elapsed":1070265,"user":{"displayName":"Gabriela Martínez","userId":"02807641605184715686"}},"outputId":"18e856ed-704b-4c43-ab05-18204da1adf6"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","Entrenando modelo con 100 neuronas en la capa oculta\n","Epoch [1/5], Pérdida de Entrenamiento: 0.2766\n","Epoch [2/5], Pérdida de Entrenamiento: 0.1241\n","Ep